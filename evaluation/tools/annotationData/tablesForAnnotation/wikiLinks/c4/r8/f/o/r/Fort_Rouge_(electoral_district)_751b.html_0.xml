</context>
<context>
	<score>0.73827934</score>
	<text>The current MLA is Jennifer Howard of the NDP.</text>
</context>
<context>
	<score>0.69749975</score>
	<text>Categories :</text>
</context>
<context>
	<score>0.6866793</score>
