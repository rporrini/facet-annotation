<context>
	<score>0.7104988</score>
	<text>Shannan is predominantly inhabited by members of the Monpa ethnic group.</text>
</context>
<context>
	<score>0.64836705</score>
	<text>[ citation needed ]</text>
</context>
<context>
	<score>0.60979</score>
